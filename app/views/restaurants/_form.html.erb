<%= form_for(restaurant) do |form| %>

  <p>
  <%= form.label :name %>
  <%= form.text_field :name %>
  </p>

  <p>
  <%= form.label :address %>
  <%= form.text_field :address %>
  </p>

  <p>
  <%= form.label :rating %>
  <%= form.number_field :rating %>
  </p>

  <p>
  <%= form.submit %>
  </p>

<% end %>